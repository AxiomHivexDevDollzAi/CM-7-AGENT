{
  "name": "cm7-agent-showcase",
  "displayName": "CM-7 Agent Showcase",
  "description": "A VS Code extension showcasing the CM-7 neutral computational module agent",
  "version": "0.0.1",
  "engines": {
    "vscode": "^1.74.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "onCommand:cm7.showDemo"
  ],
  "main": "./out/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "cm7.showDemo",
        "title": "Show CM-7 Demo"
      }
    ],
    "menus": {
      "commandPalette": [
        {
          "command": "cm7.showDemo"
        }
      ]
    }
  },
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "pretest": "npm run compile && npm run lint",
    "lint": "eslint src --ext ts",
    "test": "node ./out/test/runTest.js"
  },
  "devDependencies": {
    "@types/vscode": "^1.74.0",
    "@types/glob": "^8.1.0",
    "@types/mocha": "^10.0.1",
    "@types/node": "16.x",
    "eslint": "^8.28.0",
    "glob": "^8.0.3",
    "mocha": "^10.1.0",
    "typescript": "^4.9.4",
    "@vscode/test-electron": "^2.2.0"
  }
}